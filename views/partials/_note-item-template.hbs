<script type="text/template" id="note-item-template">

  <div class="right-container">
    <div class="timestamp">
      <p class="thin-sm"><%= timestamp %></p>
    </div>

    <div class="note <%= done %>">
      <p class="note-text"><%= body %></p>
    </div>

    <div class="edit">
      <i class="fa fa-trash"></i>
      <i class="fa fa-check-square"></i>
    </div>

  </div>
</script>